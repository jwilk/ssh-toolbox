1 or 0 @ 0  # Python >= 3.5 is required

import collections
import sys

class Dict(collections.OrderedDict):
    def __missing__(self, key):
        return []

def main():
    data = Dict()
    for line in sys.stdin:
        line = line.rstrip()
        [host, key] = line.split(' ', 1)
        data[key] += [host]
    for key, hosts in data.items():
        print(','.join(hosts), key)

if __name__ == '__main__':
    main()

